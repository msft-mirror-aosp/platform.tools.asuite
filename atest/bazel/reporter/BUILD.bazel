)

java_import(
    name = "tradefed",
    jars = [
        "//tools/tradefederation/prebuilts/filegroups/tradefed:tradefed-prebuilt",
    ],
    target_compatible_with = ["//build/bazel_common_rules/platforms/os:linux"],
)
