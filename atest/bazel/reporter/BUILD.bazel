package(default_visibility = ["//visibility:public"])

java_library(
    name = "bazel-result-reporter",
    srcs = glob(["src/**/*.java"]),
    target_compatible_with = ["//build/bazel_common_rules/platforms/os:linux"],
    deps = [
        ":tradefed",
